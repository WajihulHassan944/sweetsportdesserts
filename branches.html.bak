    <script>
        function colapseHideOrShow(id) {
            var x = document.getElementById("colapseHide"+id);
            var y = document.getElementById("colapseShow"+id);
            if (x.style.display === "none") {
                x.style.display = "block";
                y.style.display = "none";
            } else {
                x.style.display = "none";
                y.style.display = "block";
            }
        }
    </script>
